  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ts": "ts-node",
    "db:generate": "npx prisma generate --schema src/shared/libs/models/prisma/schema.prisma",
    "db:reset": "npx prisma migrate reset --schema src/shared/libs/models/prisma/schema.prisma --force --skip-generate --skip-seed",
    "db:lint": "npx prisma validate --schema src/shared/libs/models/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema ./schema.prisma --skip-generate --skip-seed"
  },
